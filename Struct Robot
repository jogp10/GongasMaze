typedef struct{
        int x;
        int y;
        int id;
    }Robot;

    vector<Robot> robots;
    for(int i=0; i<10; i++){
         Robot temp;
         temp.x = 2;
         temp.y = 4;
         temp.id = i;

         robots.push_back(temp);
    }

    for(auto i: robots){
        cout << i.id << "\n";
    }
